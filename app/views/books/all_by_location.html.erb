<% provide(:title, @location) %>
<div class="col-md-12">
    <h3>Alla tillgängliga böcker i <%= @location %> (<%= @count %>)</h3>
    <ul class="books">
      <%= render @books %>
    </ul>
    <div class="pagination">
      <%= will_paginate @books, previous_label: t("föregående"), next_label: t("nästa")%>
    </div>
</div>
