<% provide(:title, @tag) %>
<div class="col-md-12">
    <h3>Alla böcker som matchar taggen <%= link_to '#' + @tag.strip, tag_url(tags: @tag.strip)%> (<%= @count %>)</h3>
    <ul class="books">
      <%= render @books %>
    </ul>
    <%= will_paginate @books, previous_label: t("föregående"), next_label: t("nästa")%>
</div>
